{
  "name": "cheukpang/next-think",
  "description": "ThinkPHP refactor base on v3.2.3",
  "type": "library",
  "keywords": [
    "ThinkPHP",
    "refactor",
    "composer"
  ],
  "minimum-stability": "stable",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "kwan",
      "email": "303198069@qq.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "psr/simple-cache": "1.0.0",
    "psr/log": "1.0.*",
    "psr/container": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "Think\\": "src/Think",
      "Behavior\\": "src/Behavior"
    },
    "files": [
      "src/Common/function.php",
      "src/Bootstrap.php"
    ]
  },
  "support": {
    "issues": "https://github.com/xcorp1986/next-think/issues"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}